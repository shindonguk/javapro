function splitString(){
	var str = document.getElementsByName("txtData")[0].value;
	var strList = str.split(",");
		var tagList = document.getElementsByTagName("td");
	
	for(var idx=0; idx<strList.length; idx++){
		tagList[idx].innerHTML = strList[idx];
	}
}

