/*전체 화면 css*/
html {
	height: 100%;
	padding: 40px 50px 0px 50px;
	margin: 0;
	overflow: hidden;
}
body {
	height: 100%;
	padding: 40px 50px 0px 50px;
	margin: 0;
	overflow: hidden;
	background-image: url('../image/game_back.jpg');
	background-size: 100% 100%;
	background-repeat: no-repeat;"
}
#base {
	position: relative;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	width: 960px;
	height: 540px;
	margin: 0 auto;
	padding: 5px;
	background-color: #FFFFFF;
}
#left {
	position: relative;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	width: 672px;
	height: 540px;
	padding: 5px 5px 5px 5px;
}
#right {
	position: relative;
	top: -540px;
	left: 672px;
	width: 288px;
	height: 540px;
	padding: 5px 5px 5px 5px;
}

#head {
	position: relative;
	top: 0;
	left: 0;
	width: 652px;
	height: 64px;
	margin: 5px 5px 5px 5px;
	background-color: #26A8FF;
}

#logo{
	display:inline-block;
	overflow: hidden;
	width:121px;
	height:44px;
	background:url('../image/game_logo.gif') no-repeat 0 -21px;
	margin: 0 auto;
}

#blind{
	visibility: hidden;
    overflow: hidden;
    position: absolute;
    width: 0;
    height: 0;
    font-size: 0;
    line-height: 0;
}

dl {
	display: block;
	height: 54px;
	-webkit-margin-before: 1em;
	-webkit-margin-after: 1em;
	-webkit-margin-start: 0px;
	-webkit-margin-end: 0px;
}

dt {
	float: left;
	position: relative;
	height: 54px;
}

dd {
	float: left;
	position: relative;
	height: 54px;
}

#side-a-1 {
	position: relative;
	top: 0;
	left: 0;
	width: 652px;
	height: 233px;
	margin: 5px 5px 5px 5px;
}

#side-a-2 {
	position: relative;
	top: 0;
	left: 0;
	width: 652px;
	height: 233px;
	margin: 5px 5px 5px 5px;
}

#random_numArray {
	padding: 5px 5px 5px 5px;
}

.random {
	padding-left: 20px;
	width: 104px;
	height: 135px;
}

#input_numArray {
	padding: 5px 5px 0px 5px;
}

.input {
	padding-left: 21.5px;
	width: 104px;
	height: 55px;
}

.items {
	width: 300px;
	height: 43px;
}

.item {
	width: 50px;
	height: 43px;
}

#btn {
	position: relative;
	top: -203px;
	left: 534px;
	width: 200px;
	height: 243px;
}

.pad01 {
	width: 504px;
	height: 135px;
}

#side-b {
	width: 288px;
	height: 540px;
}

#result_view {
	background: #6ed155;
	margin: 10px 5px 0px 5px;
	width: 270px;
	height: 150px;
}

.display {
	width: 288px;
	height: 50px;
	margin: 13px 0px 0px 0px;
}

#result_memo {
	background-image: url('../image/record.png');
	background-size: 100% 100%;
	background-repeat: no-repeat;
	margin: 10px 0px 5px 5px;
	width: 268px;
	height: 300px;
}

.num_btn {
	font-weight: bold;
	text-decoration: none;
	font-family: Arial Black;
	box-shadow: inset #ffffff 0px 5px 8px -1px, #d2b287 1px 5px 2px;
	o-box-shadow: inset #ffffff 0px 5px 8px -1px, #d2b287 1px 5px 2px;
	-moz-box-shadow: inset #ffffff 0px 5px 8px -1px, #d2b287 1px 5px 2px;
	-webkit-box-shadow: inset #ffffff 0px 5px 8px -1px, #d2b287 1px 5px 2px;
	background: #f9e3b6;
	background: -o-linear-gradient(90deg, #f9e3b6, #f9e3b6);
	background: -moz-linear-gradient( center top, #f9e3b6 5%, #f9e3b6 100% );
	background: -webkit-gradient( linear, left top, left bottom, color-stop(0.05, #f9e3b6), color-stop(1, #f9e3b6) );
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#f9e3b6', endColorstr='#f9e3b6');
	background: -webkit-linear-gradient(#f9e3b6, #f9e3b6);
	background: -ms-linear-gradient(#f9e3b6, #f9e3b6);
	background: linear-gradient(#f9e3b6, #f9e3b6);
	text-indent: 0px;
	line-height: 33px;
	-moz-border-radius: 14px;
	-webkit-border-radius: 14px;
	border-radius: 14px;
	text-align: center;
	vertical-align: middle;
	display: inline-block;
	font-size: 31px;
	color: #d2b287;
	width: 74px;
	height: 33px;
	padding: 13px;
	text-shadow: #fff3db -1px -1px 0px;
	border-color: #f9e3b6;
	border-width: 1px;
	border-style: solid;
}

.num_btn:active {
	box-shadow: inset #ffffff 0px 5px 8px -1px, #d2b287 1px 0 2px;
	o-box-shadow: inset #ffffff 0px 5px 8px -1px, #d2b287 1px 0 2px;
	-moz-box-shadow: inset #ffffff 0px 5px 8px -1px, #d2b287 1px 0 2px;
	-webkit-box-shadow: inset #ffffff 0px 5px 8px -1px, #d2b287 1px 0 2px;
	position: relative;
	top: 5px
}

.num_btn:hover {
	background: #f9e3b6;
	background: -o-linear-gradient(90deg, #f9e3b6, #f9e3b6);
	background: -moz-linear-gradient( center top, #f9e3b6 5%, #f9e3b6 100% );
	background: -webkit-gradient( linear, left top, left bottom, color-stop(0.05, #f9e3b6), color-stop(1, #f9e3b6) );
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#f9e3b6', endColorstr='#f9e3b6');
	background: -webkit-linear-gradient(#f9e3b6, #f9e3b6);
	background: -ms-linear-gradient(#f9e3b6, #f9e3b6);
	background: linear-gradient(#f9e3b6, #f9e3b6);
}