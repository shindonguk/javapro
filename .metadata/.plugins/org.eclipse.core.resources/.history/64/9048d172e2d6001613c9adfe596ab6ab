package javastart.D01_inherit;


class Father{
	private String name;
	int age;
	public void setDate(String name, int age) {
		this.name = name;
		this.age = age;
		
	}
	public void show(){
		System.out.println("아버지 클래스");
		System.out.println("이름: "+name+",\t 나이"+age);
	}
}
