  <script type="text/javascript">
 
	window.onload=function(){
		var tds=document.getElementsByTagName("td");
		for(var idx in tds){
			tds[idx].onMouseOver=call
		}
		
		/* tds.forEach(function(obj,idx){
			obj.onMouseOver=call; */
		});
	};

	function call(){
			this.bgColor="blue";
		}

  </script>
