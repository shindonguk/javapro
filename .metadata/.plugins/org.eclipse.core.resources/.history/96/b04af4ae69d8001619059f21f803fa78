package javastart.D01_inherit;

interface Duck{
	void sound();
}


public class C02_interface {
	public static void main(String[] args) {
		/*
		 * 추상클래스
		 * overriding 처리
		 * Duck
		 * 		sound();
		 * 1) RubberDuck
		 * 			sound();
		 * 2) NormalDuck
		 * 			sound();
		 * 3) RobotDuck
		 * 			sound();
		 */
	}
}
