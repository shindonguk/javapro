package javastart.B01_object;
//person 이라는 틀을 만든다.
class person{
	String name;
	int age;
}
class product{	
	String name = "망치";
	int pcnt;//물건의 개수
	int price;//물건의 단가
	int sum;//물건의 계
	
}
/*
 
  
  
  현재고 리스트
 **/
//ex> product 물건명 데이터를 선언한 후에 main에서 호출하세요.
public class A01_objbasic {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		 * 만들어진 틀에서 person실제 메모리에 올리게 처리
		 */
		person p1 = new person();
		p1.name = "홍길동";
		person p2 = new person();
		p2.name = "동길홍";
		p1.age = 26;
		p1.age = p2.age;
		product pr1 = new product(); 
		System.out.println("이름1: "+p1.name);
		System.out.println("이름2: "+p2.name);
		System.out.println("이름2: "+p1.age);
		System.out.println("이름2: "+p2.age);
		System.out.println("물건: "+pr1.name);
	}

}
