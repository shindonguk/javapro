package javastart.B01_object;
//person 이라는 틀을 만든다.
class person{
	String name;
}

public class A01_objbasic {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		 * 만들어진 틀에서 person실제 메모리에 올리게 처리
		 */
		person p1 = new person();
		p1.name = "홍길동";
		person p2 = new person();
		p2.name = "동길홍";
	}

}
