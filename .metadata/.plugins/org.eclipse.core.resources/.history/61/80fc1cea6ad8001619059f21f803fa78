package javastart.D01_inherit;

abstract class Duck{
	private String kind;
	//접근제어자가 private 이기에 하위 클래스에서 접근할 수 있는
	//매서드를 구현하여야 한다.
	public Duck(String kind){
		this.kind=kind;
	}
	public void move(){
		System.out.println(kind+"오리 마에로");
	}
		
	 abstract void sound(){
}

class RubberDuck implements Duck{
	public RubberDuck(){
		super("고무오리");
	}
	@Override
	public void sound() {
		// TODO Auto-generated method stub
		System.out.println("고무 오리");
	}
}

class NormalDuck implements Duck{

	@Override
	public void sound() {
		System.out.println("꽥꽥");
		
	}
	
	
}


public class C02_interface {
	public static void main(String[] args) {
		/*
		 * 추상클래스
		 * overriding 처리
		 * Duck
		 * 		sound();
		 * 1) RubberDuck
		 * 			sound();
		 * 2) NormalDuck
		 * 			sound();
		 * 3) RobotDuck
		 * 			sound();
		 */
	}
}
