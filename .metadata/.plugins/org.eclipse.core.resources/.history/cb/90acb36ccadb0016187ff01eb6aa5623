<head>
<meta charset="EUC-KR">
<title>Insert title here</title>
  <script type="text/javascript">
 
	window.onload=function(){
		var tds=document.getElementsByTagName("td");
		tds.forEach(function(obj,idx){
			obj.onMouseOver=call;
		});
	};

	function call(){
			this.bgColor="blue";
		}

	}
	function change01(){
		call();
		// 클릭한 부분만 없어짐..
		this.innerHTML="";
		this.bgColor="";	}

  </script>
 </head>
